<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup Label="UserMacros">
    <test_dir>$(UserProfile)\source\test\シェル拡張のテスト\</test_dir>
  </PropertyGroup>
  <PropertyGroup>
    <OutDir>$(SolutionDir)_bin\</OutDir>
    <IntDir>$(SolutionDir)_obj\$(PlatformTarget)\$(Configuration)\$(ProjectName)\</IntDir>
  </PropertyGroup>
  <ItemGroup>
    <BuildMacro Include="test_dir">
      <Value>$(test_dir)</Value>
    </BuildMacro>
  </ItemGroup>
  <ItemDefinitionGroup>
    <ClCompile>
      <AssemblerListingLocation>$(IntDir)</AssemblerListingLocation>
    </ClCompile>
  </ItemDefinitionGroup>
  <ItemDefinitionGroup>
    <ClCompile>
      <ObjectFileName>$(IntDir)</ObjectFileName>
    </ClCompile>
    <Link>
      <ModuleDefinitionFile>main.def</ModuleDefinitionFile>
    </Link>
  </ItemDefinitionGroup>
</Project>